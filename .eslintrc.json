{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        // disable
        "no-console": "off",
        "import/no-dynamic-require": "off",
        "no-underscore-dangle": "off",
        "no-prototype-builtins": "off",
        "new-cap": "off"
    }
}